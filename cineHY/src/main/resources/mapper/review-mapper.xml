<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
"-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="reviewMapper">


	<select id="findAllReview" resultType="review" >
		SELECT
			REVIEW_NO,
			REVIEW_CONTENT,
			REVIEW_DATE,
			STAR,
			STATUS,
			USER_ID
		FROM
			TB_REVIEW
			
	
	</select>



</mapper>